{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "entry": [
    "src/app/**/*.{ts,tsx}",
    "src/app/**/route.{ts,tsx}",
    "src/app/**/page.{ts,tsx}",
    "src/app/**/layout.{ts,tsx}",
    "next.config.mjs",
    "tailwind.config.ts",
    "postcss.config.mjs"
  ],
  "project": [
    "src/**/*.{ts,tsx,js,jsx}",
    "*.{ts,tsx,js,jsx,mjs}",
    "next.config.*",
    "tailwind.config.*",
    "postcss.config.*"
  ],
  "ignore": [
    "**/*.d.ts",
    "**/node_modules/**",
    ".next/**",
    "out/**",
    "build/**",
    "dist/**",
    "public/**",
    "next-env.d.ts",
    "declarations.d.ts",
    "index.d.ts",
    "react-pdf.d.ts",
    "pdfjs-dist.d.ts",
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/*.spec.ts",
    "**/*.spec.tsx"
  ],
  "ignoreDependencies": [
    "@types/*",
    "eslint",
    "prettier",
    "@next/bundle-analyzer",
    "knip"
  ],
  "ignoreBinaries": ["lsof"],
  "ignoreExportsUsedInFile": true,
  "workspaces": {
    ".": {
      "entry": [
        "src/app/**/*.{ts,tsx}",
        "src/app/**/route.{ts,tsx}",
        "src/app/**/page.{ts,tsx}",
        "src/app/**/layout.{ts,tsx}",
        "next.config.mjs"
      ],
      "project": ["src/**/*.{ts,tsx}", "*.{ts,tsx,js,jsx,mjs}"]
    }
  },
  "next": {
    "entry": [
      "src/app/**/*.{ts,tsx}",
      "src/app/**/route.{ts,tsx}",
      "src/app/**/page.{ts,tsx}",
      "src/app/**/layout.{ts,tsx}"
    ]
  }
}
